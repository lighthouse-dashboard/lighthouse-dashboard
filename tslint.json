{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": false,
        "promise-function-async": true,
        "await-promise": true,
        "quotemark": [
            true,
            "single"
        ],
        "no-var-requires": false,
        "ordered-imports": false,
        "max-line-length": false,
        "prefer-for-of": false,
        "no-submodule-imports": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "object-literal-key-quotes": false,
        "member-access": true,
        "curly": true,
        "no-empty": true,
        "triple-equals": true,
        "prefer-const": true,
        "class-name": true
    }
}
