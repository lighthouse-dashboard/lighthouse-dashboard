{
    "name": "lighthouse-dashboard",
    "version": "5.20.4",
    "private": true,
    "description": "App to keep track of your websites by showing their lighthouse results",
    "author": {
        "email": "fabio.gianini@dreipol.ch",
        "name": "fabs",
        "url": "https://www.fabs.io"
    },
    "repository": {
        "url": "https://github.com/dreipol/lighthouse-dashboard",
        "type": "git"
    },
    "scripts": {
        "license": "npx license-checker --json > ./assets/static/licenses.json",
        "dev-server": "NODE_ENV=development node_modules/.bin/nodemon",
        "dev": "gulp",
        "storybook": "start-storybook -s ./assets -p 8000",
        "test": "npm run lint && TZ=Europe/Berlin mocha src/**/*.spec.js -r esm",
        "lint": "npm run lint-frontend && npm run lint-backend",
        "lint-frontend": "eslint -c .eslintrc ./assets/src && npx stylelint **/*.scss --config .stylelintrc.js",
        "lint-backend": "eslint -c .eslintrc ./src",
        "start": "node -r esm index.js",
        "build": "gulp build && build-storybook",
        "build-storybook": "build-storybook -c .storybook -o assets/storybook",
        "audit": "node -r esm lib/audit-scheduler/cli.js",
        "worker": "npm run cleanup && npm run audit-worker",
        "cleanup": "node -r esm lib/cleanup/cli.js",
        "audit-worker": "node -r esm lib/audit-worker/cli.js"
    },
    "size-limit": [
        {
            "path": "assets/dist/js/*.js"
        }
    ],
    "main": "./index.js",
